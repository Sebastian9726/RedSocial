v-flex xs12 sm8 offset-sm2>\n        <h2>Crea una nueva publicación</h2>\n      </v-flex>\n    </v-layout>\n\n    <!-- formulario -->\n    <v-layout row wrpa mt-3>\n      <v-flex xs12 sm6 offset-sm3>\n        <v-form v-model=\"isValidForm\" lazy-validation ref=\"form\" @submit.prevent=\"handleaddPost\">\n          <v-layout row mt-3>\n            <v-flex xs12>\n              <v-text-field\n                :rules=\"titleRules\"\n                prepend-icon=\"border_color\"\n                label=\"Título\"\n                type=\"text\"\n                v-model=\"title\"\n              ></v-text-field>\n            </v-flex>\n          </v-layout>\n\n          <v-layout row mt-3>\n            <v-flex xs12>\n              <v-text-field\n                :rules=\"urlImageRules\"\n                prepend-icon=\"insert_link\"\n                label=\"URL de la imagen\"\n                type=\"text\"\n                v-model=\"urlImage\"\n              ></v-text-field>\n            </v-flex>\n          </v-layout>\n\n          <!-- preview de imagen -->\n          <v-layout row>\n            <v-flex xs12>\n              <img :src=\"urlImage\" height=\"300px\"  />\n            </v-flex>\n          </v-layout>\n\n                  <!-- publico o privado de imagen -->\n          <v-layout row mt-3>\n            <v-flex xs12>\n                <v-checkbox\n                type=\"checkbox\"\n                label=\"Hacer publica la imagen\"\n                v-model=\"checkboxpublico\"\n              ></v-checkbox>\n            </v-flex>\n          </v-layout>\n\n          <v-layout row>\n            <v-flex xs12>\n              <v-textarea\n                auto-grow\n                :rules=\"descriptionRules\"\n                prepend-icon=\"notes\"\n                label=\"Descripción\"\n                type=\"text\"\n                v-model=\"description\"\n              ></v-textarea>\n            </v-flex>\n          </v-layout>\n        \n\n          <v-layout row mt-3>\n            <v-flex xs12>\n              <v-btn\n                :loading=\"loading\"\n                :disabled=\"!isValidForm || loading\"\n                color=\"accent\"\n                type=\"submit\"\n                light\n              >\n                <span slot=\"loader\" class=\"custom-loader\">\n                  <v-icon light>loop</v-icon>\n                </span>\n                Publicar\n              </v-btn>\n            </v-flex>\n          </v-layout>\n        </v-form>\n      </v-flex>\n    </v-layout>\n  </v-container>\n</template>\n\n<script>\nimport { mapGetters } from \"vuex\";\n\nexport default {\n  name: \"AddPost\",\n  data() {\n    return {\n      isValidForm: true,\n      title: \"\",\n      urlImage: \"\",\n      urlImage2: \"\",\n      checkboxpublico : true,\n      categories: [],\n      description: \"\",\n      titleRules: [\n        title => !!title || \"Completa este campo\",\n        title =>\n          title.length < 30 || \"El título debe tener menos de 30 caracteres\"\n      ],\n      descriptionRules: [\n        description => !!description || \"Completa este campo\",\n        description =>\n          description.length < 200 ||\n          \"La descripción debe tener menos de 200 caracteres\"\n      ],\n      urlImageRules: [urlImage => !!urlImage || \"Completa este campo\"],\n      categoriesRules: [\n        categories =>\n          categories.length >= 1 || \"Debes marcar al menos una categoría\"\n      ]\n    };\n  },\n  computed: {\n    ...mapGetters([\"loading\", \"user\"])\n  },\n  methods: {\n    handleaddPost() {\n      if (this.$refs.form.validate()) {\n        this.$store.dispatch(\"addPost\", {\n          nombre: this.title,\n          publico: this.checkboxpublico,\n          imageUrl: this.urlImage,\n          descripcion: this.description,\n          UserId: localStorage.getItem('UserId')\n        });\n      }\n    }\n  }\n};\n</script>"],"sourceRoot":"src/components/Posts"}]}